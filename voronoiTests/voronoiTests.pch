#import <Foundation/Foundation.h>
#import <simd/simd.h>

#import "VOIPoint.h"

#define AssertEqualFloats(f0, f1) do {\
XCTAssertTrue(VOIDoublesEqual(f0, f1));\
} while (0)

#define AssertEqualPoints(p0, p1) do {\
